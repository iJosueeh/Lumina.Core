{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Rest": {
        "Url": "http://+:9999",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "http://+:5001",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "Graylog": {
    "Host": "graylog",
    "Port": "12201"
  }
}